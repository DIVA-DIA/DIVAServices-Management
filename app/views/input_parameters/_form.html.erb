<%= simple_form_for([@algorithm, @input_parameter]) do |f| %>
    <%= f.input :input_type, collection: DivaServiceApi.available_input_type_keys, disabled: true %>
    <%= f.simple_fields_for :fields do |ff| %>
      <%= render partial: 'field', locals: { f: ff } %>
    <% end %>
    <%= f.submit 'Update', class: 'btn btn-primary pull-right' %>
<% end %>
