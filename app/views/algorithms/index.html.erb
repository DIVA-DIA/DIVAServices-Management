<h1>Your algorithms</h1>

<% if @algorithms.empty? %>
  <p>Upload your first algorithm <%= link_to 'here', new_algorithm_path %></p>
<% else %>
  <% @algorithms.each do |algorithm| %>
    <%= algorithm.name %>
    <br/>
    <% algorithm.input_parameters.first.fields.each do |field| %>
      -><%= field.name %>
      <% if field.type == 'ObjectField' %>
        <% field.fields.each do |field1| %>
          ->-><%= field1.name %>
        <% end %>
      <% end %>
      <br/>
    <% end %>
  <% end %>
<% end %>

<%= link_to 'New', new_algorithm_path %>
